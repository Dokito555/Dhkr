<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blue.umbrella.dhkr">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_dhikkr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_dhikkr_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".Splash"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".Quran.Surah" />
        <activity android:name=".Detail.NightDhikr" />
        <activity android:name=".Detail.MorningDhikr" />
        <activity android:name=".Dhikrs.Qauliyah_Dhikr" />
        <activity android:name=".Dhikrs.DailyDhikr" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>